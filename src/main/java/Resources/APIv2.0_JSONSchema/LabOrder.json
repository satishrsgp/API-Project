    "nextDueDate": {
      "type": "string",
      "format": "date-time",
      "description": "The next due date for this order.",
      "vendorExtensions": {
        "storage": {
          "type": "table",
          "field": "nextdue_date",
          "name": "order_schedule"
        }
      }
