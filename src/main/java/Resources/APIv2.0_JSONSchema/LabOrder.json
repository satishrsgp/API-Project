    "generalComment": {
      "type": "string",
      "maxlength": 16,
      "description": "General order comments.",
      "vendorExtensions": {
        "storage": {
          "type": "table",
          "field": "general_comment",
          "name": "lab_nor"
        }
      }
    },
    "orderComment": {
      "type": "string",
      "maxlength": 16,
