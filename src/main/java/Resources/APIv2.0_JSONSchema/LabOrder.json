    "signOffPerson": {
      "type": "string",
      "maxlength": 12,
      "description": "The user id of the person who signed off the lab order.",
      "vendorExtensions": {
        "storage": {
          "type": "table",
          "field": "sign_off_person",
          "name": "lab_nor"
        }
      }
    },
    "testStatus": {
      "type": "string",
      "maxlength": 40,
      "description": "The status of the order tests for this lab order.",
      "vendorExtensions": {
        "storage": {
          "type": "table",
          "field": "test_status",
          "name": "lab_nor"
        }
      }
    },
    "nextgenStatus": {
      "type": "string",
      "maxlength": 12,
      "description": "The status of the lab order.",
      "vendorExtensions": {
        "storage": {
          "type": "table",
          "field": "ngn_status",
          "name": "lab_nor"
        }
      }
