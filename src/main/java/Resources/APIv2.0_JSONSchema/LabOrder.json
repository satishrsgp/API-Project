  "required": [
    "id",
    "ufoNumber",
    "orderType",
    "encounterId",
    "encounterTimestamp",
    "signoffCommentsIndicator",
    "documentsIndicator",
    "isOrderedElseWhere",
    "isCompleted",
    "practiceId",
    "enterpriseId",
    "orderDate",
    "isDeleted"
