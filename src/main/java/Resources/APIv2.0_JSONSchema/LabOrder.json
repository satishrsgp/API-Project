        "storage": {
          "type": "table",
          "field": "zip",
          "name": "location_mstr"
        }
      }
    },
    "phone": {
      "type": "string",
      "maxlength": 10,
      "description": "The phone number of the service location.",
      "vendorExtensions": {
        "storage": {
          "type": "table",
          "field": "phone",
          "name": "location_mstr"
        }
      }
    },
    "fax": {
      "type": "string",
      "maxlength": 10,
      "description": "The fax number of the service location.",
      "vendorExtensions": {
        "storage": {
          "type": "table",
          "field": "fax",
          "name": "location_mstr"
        }
      }
    },
    "firstName": {
      "type": "string",
      "maxlength": 13,
      "description": "The first name of the user that signed off the lab order.",
      "vendorExtensions": {
        "storage": {
          "type": "table",
          "field": "first_name",
          "name": "user_mstr"
        }
      }
