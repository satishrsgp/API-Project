        "storage": {
          "type": "table",
          "field": "address_line_1",
          "name": "location_mstr"
        }
      }
    },
    "addressLine2": {
      "type": "string",
      "maxlength": 55,
      "description": "The second address line of the location.",
      "vendorExtensions": {
        "storage": {
          "type": "table",
          "field": "address_line_2",
          "name": "location_mstr"
        }
      }
    },
    "city": {
      "type": "string",
      "maxlength": 35,
      "description": "The city of the service location.",
