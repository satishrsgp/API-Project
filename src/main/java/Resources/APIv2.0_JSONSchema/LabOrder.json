      "vendorExtensions": {
        "storage": {
          "type": "table",
          "field": "enc_timestamp",
          "name": "lab_nor"
        }
      }
    },
    "orderDate": {
      "type": "string",
      "format": "date-time",
      "description": "The date time when the order was placed.",
      "vendorExtensions": {
        "storage": {
          "type": "table",
          "field": "create_timestamp",
          "name": "lab_nor"
        }
      }
