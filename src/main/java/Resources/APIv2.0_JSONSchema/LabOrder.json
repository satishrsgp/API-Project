        "storage": {
          "type": "table",
          "field": "order_comment",
          "name": "lab_nor"
        }
      }
    },
    "patientComment": {
      "type": "string",
      "maxlength": 16,
      "description": "Patient specific comments for this order.",
      "vendorExtensions": {
        "storage": {
          "type": "table",
          "field": "patient_comment",
          "name": "lab_nor"
        }
      }
