    "lastName": {
      "type": "string",
      "maxlength": 18,
      "description": "The last name of the user that signed off the lab order.",
      "vendorExtensions": {
        "storage": {
          "type": "table",
          "field": "last_name",
          "name": "user_mstr"
        }
      }
    },
    "middleInitial": {
      "type": "string",
      "maxlength": 1,
      "description": "The middle initial of the user that signed off the lab order.",
