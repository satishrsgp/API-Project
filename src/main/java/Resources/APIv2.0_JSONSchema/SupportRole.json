    "addressLine2": {
      "type": "string",
      "maxlength": 55,
      "vendorExtensions": {
        "storage": {
          "type": "table",
          "field": "address_line_2",
          "name": "person_support_role"
        }
      }
    },
    "city": {
      "type": "string",
