      }
    },
    "modifyTimestamp": {
      "type": "string",
      "format": "date-time",
      "vendorExtensions": {
        "storage": [
          {
            "type": "table",
            "field": "modify_timestamp",
            "name": "patient_documents"
          },
          {
            "type": "table",
            "field": "modify_timestamp",
            "name": "patient_notes"
          },
