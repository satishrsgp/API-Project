      "vendorExtensions": {
        "storage": {
          "type": "table",
          "field": "enc_id",
          "name": "patient_allergy"
        }
      }
    },
    "encounterTimestamp": {
      "type": "string",
      "format": "date-time",
      "vendorExtensions": {
        "storage": {
          "type": "table",
